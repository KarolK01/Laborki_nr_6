#include <stdio.h>

int main()
{
    int a,b;
    printf("Podaj wymiary ramki: \n");
    printf("Podaj wymiar a: \n");
    scanf("%d", &a);
    printf("\n");

    printf("Podaj wymiar b: \n");
    scanf("%d", &b);

    b=b-2; //bo liczone sÄ… wszystkie

    for (int i=0;i<a;i++){
        printf("*");
    }
    printf("\n");
    for (int i=0;i<b;i++)
    {
        printf("*");
        for (int j=0;j<a-2;j++)
        {
            printf(" ");
        }
        printf("*\n");
    }
    for (int i=0;i<a;i++)
    {
        printf("*");
    }
return 0;
}
